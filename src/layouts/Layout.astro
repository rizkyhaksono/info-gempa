---
interface Props {
  title: string
}

const { title } = Astro.props

import Header from "./Header.astro"
import Footer from "./Footer.astro"
import RandomImage from "../components/random-image"
---

<!doctype html>
<html lang="en">
  <RandomImage client:idle />
  <Header titleHeader={title} />
  <body>
    <slot />
    <Footer />
  </body>
</html>
