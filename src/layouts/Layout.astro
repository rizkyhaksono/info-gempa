---
interface Props {
  title: string
}

const { title } = Astro.props

import Header from "./Header.astro"
import Footer from "./Footer.astro"
---

<!doctype html>
<html lang="en">
  <img src="/assets/illustration.jpg" alt="Background Illustration" class="absolute -z-10 w-full" />
  <Header titleHeader={title} />
  <body>
    <slot />
    <Footer />
  </body>


</html>
